#
# Apache Superset Configuration
# This file is required for Superset to run properly
# Copy this to .env and customize for your environment
#

# General Configuration
PYTHONUNBUFFERED=1
COMPOSE_PROJECT_NAME=superset
DEV_MODE=true

# Superset Application Settings
SUPERSET_APP_ROOT="/"
SUPERSET_ENV=development
SUPERSET_LOAD_EXAMPLES=yes
SUPERSET_PORT=8088
SUPERSET_LOG_LEVEL=info
FLASK_DEBUG=false

# CRITICAL: Set this to a unique secure random value in production
# For development, you can use any string, but in production use:
# openssl rand -base64 32
SUPERSET_SECRET_KEY=YOUR_SECURE_SECRET_KEY_HERE

# Database Configuration
DATABASE_DB=superset_db
DATABASE_HOST=db
DATABASE_PORT=5432
DATABASE_DIALECT=postgresql+psycopg2
DATABASE_USER=superset
# IMPORTANT: Change this to a secure password in production
DATABASE_PASSWORD=your-secure-password-here

# PostgreSQL Root Configuration
POSTGRES_DB=superset_db
POSTGRES_USER=superset
# IMPORTANT: Change this to a secure password in production
POSTGRES_PASSWORD=your-secure-password-here

# Example Database Configuration (optional)
EXAMPLES_DB=examples_db
EXAMPLES_HOST=db
EXAMPLES_USER=examples
EXAMPLES_PASSWORD=examples
EXAMPLES_PORT=5432

# Redis Cache Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Python Path
PYTHONPATH=/app/pythonpath:/app/docker/pythonpath_dev

# Additional Settings
MAPBOX_API_KEY=''
CYPRESS_CONFIG=false
ENABLE_PLAYWRIGHT=false
PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true
BUILD_SUPERSET_FRONTEND_IN_DOCKER=true
